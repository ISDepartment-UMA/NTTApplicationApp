<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>NYY Job Application Server</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<resource-ref>
		<description>Database for NTT App</description>
		<res-ref-name>jdbc/nttDB</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
	<servlet>
		<description></description>
		<display-name>TestController</display-name>
		<servlet-name>TestController</servlet-name>
		<servlet-class>controller.test</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TestController</servlet-name>
		<url-pattern>/Test</url-pattern>
	</servlet-mapping>
</web-app>